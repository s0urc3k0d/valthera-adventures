{
  "name": "valthera-adventures",
  "version": "1.0.0",
  "description": "Bot Discord RPG basé sur l'univers Valthera et le système D&D 5E 2024",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "deploy-commands": "node src/scripts/deploy-commands.js",
    "setup-db": "node src/scripts/setup-database.js",
    "seed": "node src/scripts/seed-data.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "npm test -- --watch",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f bot"
  },
  "keywords": [
    "discord",
    "bot",
    "rpg",
    "dnd",
    "d&d",
    "5e",
    "valthera"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^14.14.1",
    "mongoose": "^8.0.3",
    "dotenv": "^16.3.1",
    "node-cron": "^3.0.3",
    "node-cache": "^5.1.2",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "prettier": "^3.1.1",
    "nodemon": "^3.0.2",
    "jest": "^29.7.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
